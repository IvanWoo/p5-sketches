{
  "name": "p5-typescript-starter",
  "author": "Yifan Wu",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "web-test-runner \"src/**/*.test.ts\"",
    "lint": "eslint . --quiet --fix --ext .ts && prettier --write src/**/*.ts",
    "new": "ts-node -P tools/tsconfig.json tools/src/new-sketch.ts",
    "set": "ts-node -P tools/tsconfig.json tools/src/set-sketch.ts",
    "doc:sketches": "ts-node -P tools/tsconfig.json tools/src/readme-sketches.ts",
    "asset:optimize": "open -a ImageOptim ./assets/"
  },
  "dependencies": {
    "@thi.ng/grid-iterators": "^0.4.37",
    "@thi.ng/idgen": "^0.2.37",
    "@thi.ng/iterators": "^5.1.65",
    "@thi.ng/random": "^2.4.2",
    "@thi.ng/strings": "^2.1.2",
    "@thi.ng/text-canvas": "^0.7.10",
    "p5": "^1.4.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/p5": "^1.3.0",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "@web/test-runner": "^0.13.15",
    "chai": "^4.3.4",
    "dateformat": "^4.5.1",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.24.1",
    "prettier": "^2.3.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.5.2",
    "vite": "^2.6.14",
    "vite-web-test-runner-plugin": "^0.1.0"
  }
}